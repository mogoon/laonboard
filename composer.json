{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-debugbar": "~2.0",
        "doctrine/dbal": "~2.0",
        "eternicode/bootstrap-datepicker": "^1.6",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "itsjavi/bootstrap-colorpicker": "^2.5",
        "ixudra/curl": "^6.12",
        "khill/lavacharts": "3.0.*",
        "larapack/config-writer": "1.*",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "mews/purifier": "^2.0",
        "phpmailer/phpmailer": "^6.0",
        "predis/predis": "^1.1",
        "socialiteproviders/kakao": "^3.0",
        "twinkleroot/social_naver": "=v3.0.1",
        "twinkleroot/laravel-modules": "=2.6.1",
        "suin/php-rss-writer": "^1.5"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "sven/flex-env": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": ["app/Helpers/globalFunctions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
